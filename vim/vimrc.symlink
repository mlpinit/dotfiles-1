call pathogen#runtime_append_all_bundles()

filetype plugin indent on

set nocompatible
set backspace=indent,eol,start
set shiftwidth=2
set tabstop=2
set expandtab
set ruler
set list
set listchars=tab:>.
set statusline=[%02n]\ %f\ %(\[%M%R%H]%)%=\ %4l,%02c%2V\ %P%*
"set number

syntax enable
set background=dark
colorscheme solarized

set clipboard=unnamed
